{
  "kind": "collectionType",
  "collectionName": "enrollments",
  "info": {
    "singularName": "enrollment",
    "pluralName": "enrollments",
    "displayName": "Enrollment"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::student.student",
      "inversedBy": "enrollments",
      "required": true
    },
    "academic_year": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::academic-year.academic-year",
      "inversedBy": "enrollments",
      "required": true
    },
    "class_standard": {
      "type": "integer",
      "min": 1,
      "max": 12,
      "required": true
    },
    "gr_no": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "date_enrolled": {
      "type": "date",
      "required": true
    },
    "administration": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::enrollment-administration.enrollment-administration",
      "mappedBy": "enrollment"
    },
    "lc_received": {
      "type": "boolean",
      "default": false
    },

    "status": {
      "type": "enumeration",
      "enum": ["Enquiry", "Waiting", "Enrolled", "Dropped"],
      "default": "Enquiry",
      "required": true
    }
  }
}