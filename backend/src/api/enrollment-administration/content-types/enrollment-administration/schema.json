{
  "kind": "collectionType",
  "collectionName": "enrollment_administrations",
  "info": {
    "singularName": "enrollment-administration",
    "pluralName": "enrollment-administrations",
    "displayName": "Enrollment Administration"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "enrollment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::enrollment.enrollment",
      "inversedBy": "administration"
    },
    "division": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::division.division"
    },
    "date_of_admission": {
      "type": "date",
      "required": true
    },
    "mode": {
      "type": "enumeration",
      "enum": [
        "online",
        "offline"
      ],
      "default": "offline"
    },
    "transport_details": {
      "type": "text",
      "private": false
    },
    "hostel_details": {
      "type": "text",
      "private": false
    },
    "fees_details": {
      "type": "text",
      "private": false
    },
    "seat_allocations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seat-allocation.seat-allocation",
      "mappedBy": "enrollment_administration"
    }
  }
}
